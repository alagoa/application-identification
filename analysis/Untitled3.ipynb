    "import numpy as np\n",
    "\n",
    "#Scalogram\n",
    "N = 30\n",
    "dt=1\n",
    "dj=1/128\n",
    "s0=2*dt\n",
    "J=1/dj * np.log2(0.5*N/s0)\n",
    "scales=s0*2**(np.arange(J)*dj)\n",
    "scales"
   ]
